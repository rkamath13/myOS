#include "include/screen.h"
#include "include/string.h"
#include "include/types.h"

kmain()
{
	ClearScreen();
	PrintChar('a');
	PrintChar('\n');
	print("this is my kernel\n");	
	uint8 l;
	string a = "hello";
	string b = "hello";
	l = stringeq(a,b);
	if(l==1)
		print("string equal\n");
	else
		print("string not equal\n"); 

}
